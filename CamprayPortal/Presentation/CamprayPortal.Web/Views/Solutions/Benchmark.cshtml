@using iTextSharp.text.pdf.qrcode
@model CamprayPortal.Web.Models.Solutions.BenchmarkViewModel
@{
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@section PageStyle{

}

@section PageScript{
    <script src="/Content/assets/global/plugins/carousel-owl-carousel/owl-carousel/owl.carousel.min.js" type="text/javascript"></script><!-- slider for products -->
    <!-- BEGIN PAGE LEVEL JAVASCRIPTS (REQUIRED ONLY FOR CURRENT PAGE) -->
    <script src="/Content/assets/frontend/layout/scripts/layout.js" type="text/javascript"></script>
    <script type="text/javascript">
        jQuery(document).ready(function () {
            Layout.init();
            Layout.initOWL();
        });
    </script>
    <!-- END PAGE LEVEL JAVASCRIPTS -->
}

<div class="main">
    <div class="container">
        <ul class="breadcrumb">
            <li><a href="/">@T("common.home")</a></li>
            <li><a href="@Url.Action("HomePage", "Solutions")">@T("common.solutions")</a></li>
            <li class="active">@T("common.benchmark")</li>
        </ul>
        <!-- BEGIN SIDEBAR & CONTENT -->
        <div class="row margin-bottom-40">
            <!-- BEGIN CONTENT -->
            <div class="col-md-12 col-sm-12">
                <div class="content-page">
                    <div class="row">
                        <ul class="nav nav-tabs">
                            <li class="active"><a data-toggle="tab" href="#tab-1">Overview</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-2">Mobile</a></li>
                            <li class=""><a data-toggle="tab" href="#tab-3">Speed Time</a></li>
                        </ul>
                        <div class="tab-content">
                            <div id="tab-1" class="tab-pane fade active in">
                                @Html.Raw(Model.Overview.Body)
                            </div>
                            <div id="tab-2" class="tab-pane fade">
                                @Html.Raw(Model.Mobile.Body)

                            </div>
                            <div id="tab-3" class="tab-pane fade">
                                @Html.Raw(Model.SpeedTime.Body)
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END SIDEBAR & CONTENT -->
    </div>
</div>